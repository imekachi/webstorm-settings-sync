<templateSet group="JavaScript">
  <template name="cl" value="console.log(`&gt; $VAR_COPY$: `, $VAR$)$END$" description="console.log( &gt;&gt; varName: varValue)" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="obj" alwaysStopAt="true" />
    <variable name="VAR_COPY" expression="VAR" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="ct" value="console.log(`&gt; $TEXT$`)$END$" description="console.log('&gt;&gt; TEXT')" toReformat="true" toShortenFQNames="true">
    <variable name="TEXT" expression="jsSuggestVariableName()" defaultValue="text" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="im" value="import $PACKAGE$ from '$PATH$'" description="import ... from '...'" toReformat="true" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="./" alwaysStopAt="true" />
    <variable name="PACKAGE" expression="jsSuggestVariableName()" defaultValue="{  }" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="=&gt;" value="($START$) =&gt; $END$" description="arrow function" toReformat="true" toShortenFQNames="true">
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="imp" value="import $PACKAGE$ from '$PATH$'" description="import ... from '...'" toReformat="true" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="./" alwaysStopAt="true" />
    <variable name="PACKAGE" expression="jsSuggestVariableName()" defaultValue="{  }" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="cg" value="console.group('&gt;&gt; $VAR$')$END$" description="console.group()" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="capitalizeAndUnderscore(String)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="cge" value="console.groupEnd()$END$" description="console.groupEnd()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="im_" value="import _ from 'lodash'" description="import _ from 'lodash'" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="imlo" value="import _ from 'lodash'" description="import _ from 'lodash'" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="cwt" value="console.warn('&gt;&gt; $TEXT$')$END$" description="console.warn('&gt;&gt; TEXT')" toReformat="true" toShortenFQNames="true">
    <variable name="TEXT" expression="jsSuggestVariableName()" defaultValue="text" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="cw" value="console.warn('&gt;&gt; $VAR_COPY$: ', $VAR$)$END$" description="console.warn( &gt;&gt; varName: varValue)" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="obj" alwaysStopAt="true" />
    <variable name="VAR_COPY" expression="VAR" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="ce" value="console.error(`&gt;&gt; $VAR_COPY$: `, $VAR$)$END$" description="console.error( &gt;&gt; varName: varValue)" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="obj" alwaysStopAt="true" />
    <variable name="VAR_COPY" expression="VAR" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="tc" value="try {&#10;  $END$&#10;} catch (error) {&#10;  console.error(`&gt;&gt; error:`, error)&#10;}" description="try catch" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="tdd" value="describe(`$FUNCTION_NAME$() - $FUNCTION_PATH$`, () =&gt; {&#10;  const specs = [&#10;    {&#10;      input: [$PARAMS$],&#10;      result: $RESULT$,&#10;    },$END$&#10;  ]&#10;&#10;  specs.forEach(({ input, result }) =&gt; {&#10;    it(`should return ${JSON.stringify(result)} when passing ${JSON.stringify(input)}`, () =&gt; {&#10;      expect($FUNCTION_NAME_COPY$(...input)).toEqual(result)&#10;    })&#10;  })&#10;})&#10;" description="TDD template" toReformat="true" toShortenFQNames="true">
    <variable name="FUNCTION_NAME" expression="jsSuggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <variable name="FUNCTION_PATH" expression="jsSuggestVariableName()" defaultValue="utils/xxx" alwaysStopAt="true" />
    <variable name="PARAMS" expression="jsArrayVariable()" defaultValue="" alwaysStopAt="true" />
    <variable name="RESULT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FUNCTION_NAME_COPY" expression="FUNCTION_NAME" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
      <option name="JS_CLASS" value="false" />
      <option name="JS_EXPRESSION" value="false" />
    </context>
  </template>
  <template name="imcrl" value="import colorLog from 'shared/utils/colorLog'" description="import colorLog" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ctb" value="console.group(`&gt; $VAR_COPY$: `)&#10;console.table($VAR$)$END$&#10;console.groupEnd()" description="console.table( &gt; varName, varValue)" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="obj" alwaysStopAt="true" />
    <variable name="VAR_COPY" expression="VAR" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
</templateSet>